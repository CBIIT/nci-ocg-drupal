@import "base";


/*
 * Admin Menu Override
 */
#admin-menu{
	margin: 0;
}


/*
 * Body
 */

body {
  margin: 0;
  padding: 0;
  color: #323232;
  font-weight: normal;
  background-image: url(../images/bkg-tile.png);
  background-repeat: repeat;
}

a:focus{
  outline: 1px dotted $callout-red; 
}

