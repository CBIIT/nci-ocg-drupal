    .right{
      width: 52%;
      padding-left: 0;
    }
    
    p{
      margin-bottom: 5px;
    }
  }
  
  .views-row{
    position: relative;
    border-bottom: 1px dotted #d7d7d7;
    padding-bottom: 25px;
    margin-bottom: 22px !important;
    
    .data-matrix{
      top: 0;
      left: 230px;
    }
    
    .left img{
      box-shadow: 3px 5px 7px #b4b4b4;
    }
    
    h2{
      margin-top: 5px;
      font-size: $h4-font-size;
      line-height: 1.1em;
    }
    
     h3 a{
      span{
        color: $callout-red;
      }
    }
  }
  
  .views-row-last{
    border-bottom: none;
  }
}

.section-programs .view-news-and-publications .views-row{
  padding-bottom: 0;
}

.intro p{
  font-size: 15px;
}

.archive.last .view{
  border-bottom: none;
}

.two-sidebars,
.sidebar-first{
  #main{
    background: #fff url(../images/middleshadow.png) repeat-y 15px top;
  }
}

.two-sidebars,
.sidebar-second{
  .region-sidebar-second{
   padding-left: 0;
   width: 220px;
 }
}

/*
 * Individual Program Landing Page Custom Styles
 */
#block-views-program-header-block{
  overflow: visible;
  
  .view-program-header{
    overflow: visible;
    
    .views-field-field-image{
      height: 310px;
        img{
        position: absolute;
        margin-left: -20px;
        box-shadow: 3px 5px 7px #b4b4b4;
      }
    }
    
    .data-matrix{
      width: 220px;
    }
    
    .corner{
      width: 190px;
    }
  }
}

.view-program-header{
  position: relative;
  
  h1{
    color: $link-brown;
    text-transform: capitalize;
    span{
      color: $callout-red;
    }
  }
  
  .data-matrix{
    width: 220px;
    height: 170px;
    padding: 0;
    a{
      text-decoration: none;
      font-size: $h5-font-size;
      width: 100%;
      height: 170px;
      display: block;
      background-image: url(../images/program-matrix.png);
      background-position: center 40px;
      background-repeat: no-repeat;
      text-align: center;
      padding: 15px 0 0 0;
      &:hover{
        background-image: url(../images/program-matrix.png);
        background-position: center -110px;
      }
    }
  }
    
  .views-field-title{
    h1{
      font-size: 2em;
      margin-top: 25px;
      margin-bottom: 5px;
    }
  }
  
  .corner{
   position:relative;
   width:195px !important;
   height: 128px;
   float: right;
   padding:12px 5px 0 20px;
   position: relative;
   overflow: hidden; 
   background-color: #776349;
   color: #fff;
   margin-top: -140px;
   font-size: 12px;
     
     &:before{
      content: "";
      position: absolute;
      bottom: 0;
      right: 0;
      border-width: 23px 23px 0 0;
      border-style: solid;
      border-color: transparent #fff transparent #fff;
      display: block;
      width: 0;
    }
    
    p{
      line-height: 19px;
      margin-bottom: 7px;
    }
    
    a{
      color: #fff;
      text-decoration: none;
      font-size: 16px;
      font-family: $arial;
      font-weight: 700;
      .fontface & {
        font-family: $franklin;
        font-weight: normal;
      }
      &:hover{
        color: #fff !important;
        text-decoration: underline;
      }
    }
  }
}

.views-field-field-program-init-comp-summary .corner{
  height: 300px;
  margin-top: -310px;
}

/*
 * Publications Pages Custom Styles
 */
.page-node-21 .more-link{
  right: 15px;
}

.page-node-21 .node-21 .field-name-body p{
  font-size: 15px;
  margin-top: -20px;
} 

.page-node-21 .view-news-and-publications{
  border-bottom: 5px solid #e1e1e1;
}

.page-node-21 .view-news-and-publications .right{
  width: 88.5%;
  padding-left: 0;  
}

.page-news-publications .view-news-and-publications{
  .right{
    width: 80%;
  }
  
  h2{
    text-transform: capitalize;
    line-height: 26px;
    margin-top: 0;
    margin-bottom: 0;
    font-size: $h4-font-size;
    line-height: 1em;
  }
  
  p{
    margin-bottom: 5px;
  }
  .views-row{
    padding-bottom: 0 !important;
  }  
}

.front .view-news-and-publications{
 .right{
   width: 360px;
   padding-left: 0;
 }  
}

.page-programs .view-publications,
.page-news-publications-publications .view{
  .views-field-field-programs-abbreviated-title .left{
    width: 150px;
  }
  .right{
    width: 300px;
    padding: 0;
    h2{
      margin-top: 0;
    }
  }
  
  p{
    margin-bottom: 5px;
  } 
  
  img{
    box-shadow: 3px 5px 7px #b4b4b4;
  }
}

/*
 * Node Pages Custom Styles
 */

.field-type-entityreference{
  overflow: hidden;
  
  .field-items{
    float: none;
  }
  
  .views-label,
  .field-label{
    color: #7d7d7d;
    font-weight: bold;
    text-transform: uppercase;
    float: left;
    padding: 5px 10px 5px 0;
  } 
  
  a{
    background-color: #7d7d7d;
    padding: 2px 10px;
    display: block;
    color: #fff !important;
    font-weight: bold;
    margin: 3px 10px 3px 0;
    float: left;
    font-size: 12px;
    width: auto;
    
    &:hover{
      background-color: $callout-red; 
      color: #fff !important;
      text-decoration: none !important;
    }
  }
}

.views-field-field-resource-resource-type.field-type-entityreference .field-content{
  background-color: #7d7d7d;
  padding: 2px 10px;
  font-size: 12px;
  color: #fff !important;
  font-weight: bold;
  margin: 3px 10px 3px 0;
  float: left;
}

.field-name-field-publication-journal .field-items{
  font-style: italic;
}

.view-mode-full .field-type-image{
  float: left;
  padding-right: 20px;
  padding-bottom: 20px;
}

.field-name-field-news-ext-link{
  padding-bottom: 10px;
}

span.noicon{
  text-indent: -999999px;
  display: inline-block;
  margin-left: 10px;
  width: 14px;
  position: absolute;
}

span.newtab{
  text-indent: -999999px;
  background: url(../images/newtab.png) no-repeat;
  display: inline-block;
  width: 14px;
}

/*
 * Send Page by Email Styles
 */
#print-mail-form{
  input[type="text"]{
    padding: 3px 5px;
    border: none;
    border: 1px solid #cfcfcf;
    box-shadow: inset 0px 1px 3px #404040;
  }
  
  textarea{
    max-width: 445px;
    padding: 3px 5px;
    border: none;
    border: 1px solid #cfcfcf;
    box-shadow: inset 0px 1px 3px #404040;
  }
  
  .description{
    margin-left: 170px;
  }
  
  textarea#edit-txt-message{
    max-width: none;
  }
  
  input[type="reset"],
  input[type="submit"]{
    -webkit-appearance: none;
    background: none;
    border: none;
    font-family: $arial;
    font-weight: 700;
    color: $callout-red;
    text-transform: uppercase;
    font-size: 14px;
    .fontface & {
      font-family: $franklinbold;
      font-weight: normal;
    }
    &:hover{
      text-decoration: underline;
    }
  }
}





