}

#page {
  font-weight: normal;
}

.not-front .page-node #content img{
  box-shadow: none;
  float: left;
  padding-right: 20px;
  padding-bottom: 15px;
}

/*
 * The skip-link link will be completely hidden until a user tabs to the link.
 * See the element-focusable rule in system.base.css.
 */

#skip-link {
  margin: 0;
  background-color: #767676;

  a,
  a:visited {
    display: block;
    width: 100%;
    padding: 2px 0 3px 0;
    text-align: center;
    background-color: #767676;
    color: #fff;
    font-size: 16px;
  }
}

.left{
