  li.pager-last{
    background-image: url(../images/last.gif);
    background-position: right center;
    background-repeat: no-repeat;
    padding-right: 15px;
  }
  
  li.pager-previous{
    background-image: url(../images/previous.gif);
    background-position: left center;
    background-repeat: no-repeat;
    padding-left: 10px;
    padding-right: 0;
  }
  
