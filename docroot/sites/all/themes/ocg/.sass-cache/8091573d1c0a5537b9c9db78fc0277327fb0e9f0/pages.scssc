    }
    .program-all-programs{
      width: 125px;
      text-transform: uppercase;
      a{
        color: #fff;
      }
    }
  }
}

.view-publications .program-tag{
  float: none;
  margin-bottom: 10px;
}

.page-news-publications-news .program-tag{
  padding: 0;
}


 
/*
 * Programs Landing Page Custom Styles
 */

.archive{
  .view{
    padding-bottom: 4px;
    border-bottom: 5px solid #e1e1e1 ;
    
    .right{
      width: 52%;
      padding-left: 0;
    }
    
    p{
      margin-bottom: 5px;
    }
  }
  
  .views-row{
    position: relative;
    border-bottom: 1px dotted #d7d7d7;
    padding-bottom: 25px;
    margin-bottom: 22px !important;
    
    .data-matrix{
      top: 0;
      left: 230px;
    }
    
    .left img{
      box-shadow: 3px 5px 7px #b4b4b4;
    }
    
    h2{
      margin-top: 5px;
      font-size: $h4-font-size;
      line-height: 1.1em;
    }
    
     h3 a{
      span{
        color: $callout-red;
      }
    }
  }
  
  .views-row-last{
    border-bottom: none;
  }
}

.section-programs .view-news-and-publications .views-row{
  padding-bottom: 0;
}

.intro p{
  font-size: 15px;
}

.archive.last .view{
  border-bottom: none;
}

.two-sidebars,
.sidebar-first{
  #main{
    background: #fff url(../images/middleshadow.png) repeat-y 15px top;
  }
}

.two-sidebars,
.sidebar-second{
  .region-sidebar-second{
   padding-left: 0;
   width: 220px;
 }
}

/*
 * Individual Program Landing Page Custom Styles
 */
#block-views-program-header-block{
  overflow: visible;
  
  .view-program-header{
    overflow: visible;
    
    .views-field-field-image{
      height: 310px;
        img{
        position: absolute;
        margin-left: -20px;
      }
    }
    
    .data-matrix{
      width: 220px;
    }
    
    .corner{
      width: 190px;
    }
  }
}

.view-program-header{
  position: relative;
  
  h1{
    color: $link-brown;
    text-transform: capitalize;
    span{
      color: $callout-red;
    }
  }
  
  .data-matrix{
    width: 220px;
    height: 170px;
    padding: 0;
    a{
      text-decoration: none;
      font-size: $h5-font-size;
      width: 100%;
      height: 170px;
      display: block;
      background-image: url(../images/program-matrix.png);
      background-position: center 40px;
      background-repeat: no-repeat;
      text-align: center;
      padding: 15px 0 0 0;
      &:hover{
        background-image: url(../images/program-matrix.png);
        background-position: center -110px;
      }
    }
  }
    
  .views-field-title{
    h1{
      font-size: 2em;
      margin-top: 25px;
      margin-bottom: 5px;
    }
  }
  
  .corner{
   position:relative;
   width:195px !important;
   height: 128px;
   float: right;
   padding:12px 5px 0 20px;
   position: relative;
   overflow: hidden; 
   background-color: #776349;
   color: #fff;
   margin-top: -140px;
   font-size: 12px;
     
     &:before{
      content: "";
      position: absolute;
      bottom: 0;
      right: 0;
      border-width: 23px 23px 0 0;
      border-style: solid;
      border-color: transparent #fff transparent #fff;
      display: block;
      width: 0;
    }
    
    p{
      line-height: 19px;
      margin-bottom: 7px;
    }
    
    a{
      color: #fff;
      text-decoration: none;
      font-size: 16px;
      font-family: $arial;
      font-weight: 700;
      .fontface & {
        font-family: $franklin;
        font-weight: normal;
      }
      &:hover{
        color: #fff !important;
        text-decoration: underline;
      }
    }
  }
}

.views-field-field-program-init-comp-summary .corner{
  height: 300px;
  margin-top: -310px;
}

/*
 * Publications Pages Custom Styles
 */
.page-node-21 .more-link{
  right: 15px;
}

.page-node-21 .view-news-and-publications{
  border-bottom: 5px solid #e1e1e1;
}

.page-node-21 .view-news-and-publications .right{
  width: 88.5%;
  padding-left: 0;  
}

.page-news-publications .view-news-and-publications{
  .right{
    width: 80%;
  }
  
  h2{
    text-transform: capitalize;
    line-height: 26px;
    margin-top: 0;
    margin-bottom: 0;
    font-size: $h4-font-size;
    line-height: 1em;
  }
  
  p{
    margin-bottom: 5px;
  }
  .views-row{
    padding-bottom: 0 !important;
  }  
}

.front .view-news-and-publications{
 .right{
   width: 360px;
   padding-left: 0;
 }  
}

.page-programs .view-publications,
.page-news-publications-publications .view{
  .views-field-field-programs-abbreviated-title .left{
    width: 150px;
  }
  .right{
    width: 300px;
    padding: 0;
    h2{
      margin-top: 0;
    }
  }
  
  p{
    margin-bottom: 5px;
  } 
  
  img{
    box-shadow: 3px 5px 7px #b4b4b4;
  }
}

/*
 * Node Pages Custom Styles
 */

.field-type-entityreference{
  overflow: hidden;
  
  .field-items{
    float: none;
  }
  
  .views-label,
  .field-label{
    color: #7d7d7d;
    font-weight: bold;
    text-transform: uppercase;
    float: left;
    padding: 5px 10px 5px 0;
  } 
  
  a{
    background-color: #7d7d7d;
    padding: 1px 10px 0;
/*     text-transform: uppercase; */
    line-height: 19px;
    display: block;
    color: #fff !important;
    font-weight: bold;
    margin: 3px 10px 3px 0;
    float: left;
    font-size: 12px;
    width: auto;
    
    &:hover{
      background-color: $callout-red; 
      color: #fff !important;
      text-decoration: none !important;
    }
  }
}

.views-field-field-resource-resource-type.field-type-entityreference .field-content{
  background-color: #7d7d7d;
  padding: 2px 10px;
  font-size: 12px;
  color: #fff !important;
  font-weight: bold;
  margin: 3px 10px 3px 0;
  float: left;
}

.field-name-field-publication-journal .field-items{
  font-style: italic;
}

.view-mode-full .field-type-image{
  float: left;
  padding-right: 20px;
  padding-bottom: 20px;
}

.field-name-field-news-ext-link{
  padding-bottom: 10px;
}

span.noicon{
  text-indent: -999999px;
  display: inline-block;
  margin-left: 10px;
  width: 14px;
  position: absolute;
}

span.newtab{
  text-indent: -999999px;
  background: url(../images/newtab.png) no-repeat;
  display: inline-block;
  width: 14px;
}

span.pdf{
  text-indent: -999999px;
  background: url(../images/pdf.gif) no-repeat;
  display: inline-block;
  width: 14px;
  margin-left: 3px;
}


/*
 * Send Page by Email Styles
 */
#print-mail-form{
  input[type="text"]{
    padding: 3px 5px;
    border: none;
    border: 1px solid #cfcfcf;
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    width: 445px;
  }
  
  textarea{
    width: 445px;
    margin-left: 174px;
    padding: 3px 5px;
    border: none;
    border: 1px solid #cfcfcf;
  }
  .grippie{
    width: 445px;
    margin-left: 174px;
  }
  .description{
    margin-left: 174px;
  }
  
  textarea#edit-txt-message{
    max-width: none;
  }
  
  input[type="reset"],
  input[type="submit"]{
    -webkit-appearance: none;
    border: none;
    font-family: $arial;
    font-weight: 700;
    color: #fff;
    text-transform: uppercase;
    font-size: 14px;
    padding: 5px 15px 5px;
    margin-right: 10px;
    background: #796245;
    background: -moz-linear-gradient(top, #796245 1%, #7b664c 40%, #624f38 100%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(1%, #796245), color-stop(40%, #7b664c), color-stop(100%, #624f38));
    background: -webkit-linear-gradient(top, #796245 1%, #7b664c 40%, #624f38 100%);
    background: -o-linear-gradient(top, #796245 1%, #7b664c 40%, #624f38 100%);
    background: -ms-linear-gradient(top, #796245 1%, #7b664c 40%, #624f38 100%);
    background: linear-gradient(to bottom, #796245 1%, #7b664c 40%, #624f38 100%);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#796245', endColorstr='#624f38',GradientType=0 );
    -ms-filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#796245', endColorstr='#624f38',GradientType=0 );
    .fontface & {
      font-family: $franklin;
      font-weight: normal;
    }
    &:hover{
      background: -moz-linear-gradient(top, #990000 0%, #a22727 40%, #810202 100%);
      background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #990000), color-stop(40%, #a22727), color-stop(100%, #810202));
      background: -webkit-linear-gradient(top, #990000 0%, #a22727 40%, #810202 100%);
      background: -o-linear-gradient(top, #990000 0%, #a22727 40%, #810202 100%);
      background: -ms-linear-gradient(top, #990000 0%, #a22727 40%, #810202 100%);
      background: linear-gradient(to bottom, #990000 0%, #a22727 40%, #810202 100%);
      filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#990000', endColorstr='#810202',GradientType=0 );
      -ms-filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#990000', endColorstr='#810202',GradientType=0 );
    }
  }
}





