 * @file
 * Page Styling
 *
 * Style the markup found in page.tpl.php. Also includes some styling of
 * miscellaneous Drupal elements that appear in the $content variable, such as
 * ul.links, .pager, .more-link, etc.
 */

@import "base";


/*
 * Admin Menu Override
 */
#admin-menu{
	margin: 0;
}


/*
 * Body
 */

body {
  margin: 0;
  padding: 0;
  color: #323232;
  font-weight: normal;
  background-image: url(../images/bkg-tile.png);
  background-repeat: repeat;
}

:focus{
  outline: 1px dotted $callout-red; 
}

.region-footer ul li a:focus,
.region-navigation ul li a:focus{
  outline: 1px dotted #fff!important;
  background-color: transparent !important;
}

#page {
  font-weight: normal;
}

.not-front #content img{
  box-shadow: 3px 5px 7px #b4b4b4;
}

.not-front .page-node #content img{
  box-shadow: none;
  float: left;
  padding-right: 20px;
  padding-bottom: 15px;
}

/*
 * The skip-link link will be completely hidden until a user tabs to the link.
 * See the element-focusable rule in system.base.css.
 */

#skip-link {
  margin: 0;
  background-color: #767676;

  a,
  a:visited {
    display: block;
    width: 100%;
    padding: 2px 0 3px 0;
    text-align: center;
    background-color: #767676;
    color: #fff;
    font-size: 16px;
  }
}

.left{
  float: left;
  padding-right: 20px;
}

.right{
  float: right;
  padding-left: 15px;
}

/*
 * Header
 */
.NCI-banner{
  background: #5c5c5c; /* Old browsers */
  background: -moz-linear-gradient(top,  #5c5c5c 0%, #707070 100%); /* FF3.6+ */
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#5c5c5c), color-stop(100%,#707070)); /* Chrome,Safari4+ */
  background: -webkit-linear-gradient(top,  #5c5c5c 0%,#707070 100%); /* Chrome10+,Safari5.1+ */
  background: -o-linear-gradient(top,  #5c5c5c 0%,#707070 100%); /* Opera 11.10+ */
  background: -ms-linear-gradient(top,  #5c5c5c 0%,#707070 100%); /* IE10+ */
  background: linear-gradient(to bottom,  #5c5c5c 0%,#707070 100%); /* W3C */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#5c5c5c', endColorstr='#707070',GradientType=0 ); /* IE6-8 */
  -ms-filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#5c5c5c', endColorstr='#707070',GradientType=0 ); /* IE8+ */
}

.NCI-inner{
  overflow: hidden;
  min-width: 960px;
  
  a{
    display: block;
  }
}
 
.dna-banner{
  background-image: url(../images/dna-sequence.png);
  background-repeat: repeat-x;
  height: 20px;
  min-width: 960px;
}


.outer-header{
  max-width: none;
  background-image: url(../images/continue-bkg.png);
  background-repeat: repeat;
  min-width: 960px;
  height: 161px;
}


.disappear{ 
  background: url(../images/header.png) no-repeat center top;
}

#header {
  margin-bottom: -3px;
  background-image: url(../images/header-bkg.png);
  background-repeat: no-repeat;
  position: relative;
  padding: 0 20px;
}

#logo { /* Wrapping link for logo */
  float: left; /* LTR */
  margin: 0;
  padding: 0 20px;
  margin-top: 35px;
  margin-bottom: 35px;

  img {
    vertical-align: middle;
  }
}

#name-and-slogan { /* Wrapper for website name and slogan */
  position: absolute;
  top: 0;
  left: -99999999px;
}

#site-name { /* The name of the website */
  
  .of{
  font-style: italic;
  }
  
  a:link,
  a:visited {
    color: #473121;
    text-decoration: none;
    display: block;
  }

  a:hover
  a:focus {
    text-decoration: underline;
  }
}

#site-slogan { /* The slogan (or tagline) of a website */
  margin: 0;
  font-size: 1em;
}

.region-header { /* Wrapper for any blocks placed in the header region */
  clear: right; /* Clear the logo */
  float: right;
  width: 430px;
  padding-right: 20px;
  overflow: hidden;
}

/*
 * Main (container for everything else)
 */
.main-wrap{
  background: -moz-linear-gradient(top,  rgba(255,255,255,0) 3%, rgba(255,245,155,0) 70%, rgba(255,241,112,0.25) 99%, rgba(255,241,110,0.25) 100%); /* FF3.6+ */
  background: -webkit-gradient(linear, left top, left bottom, color-stop(3%,rgba(255,255,255,0)), color-stop(70%,rgba(255,245,155,0)), color-stop(99%,rgba(255,241,112,0.25)), color-stop(100%,rgba(255,241,110,0.25))); /* Chrome,Safari4+ */
  background: -webkit-linear-gradient(top,  rgba(255,255,255,0) 3%,rgba(255,245,155,0) 70%,rgba(255,241,112,0.25) 99%,rgba(255,241,110,0.25) 100%); /* Chrome10+,Safari5.1+ */
  background: -o-linear-gradient(top,  rgba(255,255,255,0) 3%,rgba(255,245,155,0) 70%,rgba(255,241,112,0.25) 99%,rgba(255,241,110,0.25) 100%); /* Opera 11.10+ */
  background: -ms-linear-gradient(top,  rgba(255,255,255,0) 3%,rgba(255,245,155,0) 70%,rgba(255,241,112,0.25) 99%,rgba(255,241,110,0.25) 100%); /* IE10+ */
  background: linear-gradient(to bottom,  rgba(255,255,255,0) 3%,rgba(255,245,155,0) 70%,rgba(255,241,112,0.25) 99%,rgba(255,241,110,0.25) 100%); /* W3C */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#00ffffff', endColorstr='#40fff16e',GradientType=0 ); /* IE6-8 */
  -ms-filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#00ffffff', endColorstr='#40fff16e',GradientType=0 ); /* IE8+ */
  width: 100%;
  
  a:hover{
    color: $callout-red !important;
    text-decoration: underline !important;
  }
}

#main {
  background-color: #fff;
  margin-top: 2px;
}

.front #main{
  background: none;
}

#pre-content {
  padding: 0 20px;
  padding-top: 1.6em;
}

.not-front #pre-content{
  background: #fff;
}

/*
 * Content
 */
.front #main #postscript-first,
.front #main #postscript-second,
#main #content {
  padding-bottom: 30px;
  
  a{
    color: $link-brown;
    text-decoration: none;
    &:visited{
        color: $link-visited;
      }
      
      &:hover{
        color: $callout-red;
      }
      
      &:active{
        color: $callout-red;
      }
  }
  
  ul.tabs{
    li{
      list-style-image: none;
    }
  }
    
  ul{
    list-style-type: none;
    list-style-image: url(../images/bullet.gif);
    margin-top: 0;
    margin-bottom: 1.5em;
  }
  
  .even{
    padding-right: 0;
  }
}

.not-front #main #content{
  position: relative;
  min-height: 700px;
  p a{
    text-decoration: underline;
  }
}

.region-highlighted {
}

.breadcrumb { /* The path to the current page in the form of a list of links */
  padding-bottom: 30px;
  
  ol {
    margin: 0;
    padding: 0;
  }
  li {
    display: inline;
    list-style-type: none;
    margin: 0;
    padding: 0;
    font-size: 12px;
    color: #616161;
    
    a{
      color: #313131;
      text-decoration: none;
    }
  }
}

h1.title, /* The title of the page */
h2.node-title, /* Title of a piece of content when it is given in a list of content */
h2.block-title, /* Block title */
h2.title, /* Comment section heading */
h2.comment-form, /* Comment form heading */
h3.title { /* Comment title */
  margin-top: 0;
  color: $callout-red;
  font-family: $franklin;
  line-height: 30px;
  
  a{
    color: $link-brown;
    text-decoration: none;
    font-weight: normal;
  }
}

h2.block-title{
  margin: 0;
  line-height: 25px;
  text-transform: uppercase !important;
}

h1{
  text-transform: uppercase;
  margin-bottom: 12px;
  font-family: $franklin;
  font-weight: normal;
  line-height: 30px;
}

h2{
  margin-bottom: 5px;
  text-transform: uppercase;
  font-family: $franklin;
  font-weight: normal;
  line-height: 28px;
  color: $callout-red;
}

h3, h4{
  margin-bottom: 5px;
  font-family: $franklin;
  font-weight: normal;
  line-height: 24px;
  color: $callout-red;
  a{
    font-weight: normal;
  }
}

.not-front h1.title,
.not-front h2{
  text-transform: none;
}

.page-node-22 h1.title,
.page-node-211 h1.title,
.page-node-1 h1.title,
.page-node-21 h1.title{
