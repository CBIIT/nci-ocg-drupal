@import "base";


/*
 * Views' admin tabs
 *
 * The views admin tabs use the same classes as the secondary tabs. We prevent
 * some of our tab styling from bleeding into (and breaking) the Views admin
 * pages. If your sub-theme isn't used for admin pages, you can remove this
 * entire section.
 */
.views-displays .secondary {
  li {
    float: none;
    margin: 0 6px 0;
  }

  a:link,
  a:visited {
    @include single-text-shadow(none);
    padding: 2px 7px 3px;
  }

  .open > a {
    @include border-radius(7px 7px 0 0);
    &:hover {
      color: #333;
    }
  }

  .action-list li {
    margin: 0;
    &:first-child {
      @include border-radius(0 7px 0 0);
    }
    &:last-child {
      @include border-radius(0 0 7px 7px);
    }
  }
}

.view{
  overflow: hidden;
  .views-row{
    margin-bottom: 10px;
  }
}

/* background corner not showing on firefox */

.corner .views-row{
   position:relative;
   width:230px;
   float: left;
   overflow: hidden;
   background:  #f2f2f2;
   background:
        -webkit-linear-gradient(225deg,  rgba(242,242,242,1), rgba(242,242,242,1)),
        -webkit-linear-gradient(45deg, rgba(242,242,242,1), rgba(242,242,242,1)),
        -webkit-linear-gradient(135deg, transparent 20px,  rgba(242,242,242,1) 15px);
    background:
        -moz-linear-gradient(225deg,  rgba(242,242,242,1), rgba(242,242,242,1)),
        -moz-linear-gradient(45deg, rgba(242,242,242,1), rgba(242,242,242,1)),
        -moz-linear-gradient(135deg, transparent 20px,  rgba(242,242,242,1) 15px);
    background:
        -o-linear-gradient(225deg,  rgba(242,242,242,1), rgba(242,242,242,1)),
        -o-linear-gradient(45deg, rgba(242,242,242,1), rgba(242,242,242,1)),
        -o-linear-gradient(135deg, transparent 20px,  rgba(242,242,242,1) 15px);
    background-image:
        linear-gradient(225deg,  rgba(242,242,242,1), rgba(242,242,242,1)),
