    -ms-filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#c45208', endColorstr='#e3830c',GradientType=1 ); /* IE8+ */
    background-size: 100%;

    
    &:before{
      border-color: #E3830C #fcfeff #E3830C #fcfeff;
