    background: #bb4418; /* Old browsers */
    background : -webkit-linear-gradient(top, rgb(213, 118, 34) 01%, rgb(208, 108, 32) 40%, rgb(187, 68, 24) 99%); /* Chrome 10+,Safari */ 
    background : -moz-linear-gradient(top, rgb(213, 118, 34) 01%, rgb(208, 108, 32) 40%, rgb(187, 68, 24) 99%); /* Firefox 4+ */
    background : -o-linear-gradient(top, rgb(213, 118, 34) 01%, rgb(208, 108, 32) 40%, rgb(187, 68, 24) 99%); /* Opera */
    background : -ms-linear-gradient(top, rgb(213, 118, 34) 01%, rgb(208, 108, 32) 40%, rgb(187, 68, 24) 99%); /* IE 10+ */ 
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#d57622', endColorstr='#bb4418',GradientType=0 ); /* IE6-8 */
    -ms-filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#d57622', endColorstr='#bb4418',GradientType=0 ); /* IE8+ */
    .fontface & {
      font-family: $franklin;
      font-weight: normal;
    }
  }
  
  .views-row .field-content{
    padding: 5px 20px;
    margin-bottom: 0;
  }
  
  .views-row .views-field-field-issue-date .field-content{
    padding: 0;
  }
  
  p.field-content{
    font-family: $helvetica;
  }
  
  h3.field-content{
    color: #767676;
  }
  
  .views-field-title-1 a{
    color: $callout-red !important;
  }
  
  .views-field-title p{
    color: $link-brown;
    font-size: 15px;
    font-family: $arial;
    font-weight: 700;
    .fontface & {
      font-family: $franklin;
      font-weight: normal;
    }
  }
  
  .views-row{
    padding-bottom: 20px;
  }
}

/*
 * Resources Archive View Styles
 */

.views-field-field-resource-link{
  padding-bottom: 10px;
  h3{
    font-size: $h4-font-size;
  }
}

/*
 * Last Updated View Styles
 */

.view-last-updated{
  font-family: $helvetica;
  font-size: 12px;
  color: #646464;
}

/*
 * Program tag with Abbreviated Title View Styles
 */

.view-program-abbreviation{
  float: left;
  clear: both;
  color: #7d7d7d;
  font-weight: bold;
  text-transform: uppercase;
  padding: 5px 10px 5px 0;
  a{
    width: 70px;
    display: inline-block;
    background-color: #767676;
    text-align: center;
    color: #fff !important;
    font-weight: bold;
    margin-right: 10px; 
    &:hover{
      color: #fff !important;
    }
  }
}














