 */

.front .view-announcement,
.view-announcement{
  position: relative;
  .views-row{
    width: 880px;
    padding: 20px;
    background-repeat: repeat;
    margin-bottom: 0;
    background: #c45208; /* Old browsers */
    background: -moz-linear-gradient(left,  #c45208 0%, #e3830c 75%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, right top, color-stop(0%,#c45208), color-stop(75%,#e3830c)); /* Chrome,Safari4+ */ 
    background: -webkit-linear-gradient(left,  #c45208 0%, #e3830c 75%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(left,  #c45208 0%,#e3830c 75%); /* Opera 11.10+ */
    background: -ms-linear-gradient(left,  #c45208 0%,#e3830c 75%); /* IE10+ */
    background: linear-gradient(to right,  #c45208 0%,#e3830c 75%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#c45208', endColorstr='#e3830c',GradientType=1 ); /* IE6-9 */
    -ms-filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#c45208', endColorstr='#e3830c',GradientType=1 ); /* IE8+ */
    background-size: 100%;

    
    &:before{
      border-color: #E3830C #fcfeff #E3830C #fcfeff;
      content: "";
      position: absolute;
      bottom: 0;
      right: 0;
      border-width: 23px 23px 0 0;
      border-style: solid;
      display: block;
      width: 0;
    }
  }
  
  span{
    font-family: $arial;
    font-size: $h2-font-size; 
    font-weight: 700;
    text-transform: uppercase;
    float: left;
    width: 44%;
    color: #fff;
    background-image: url(../images/alert.png);
    background-repeat: no-repeat;
    padding-left: 40px;
    line-height: 30px;
    min-height: 40px;
    .fontface & {
      font-family: $franklin;
      font-weight: normal;
    }
  }
  
  .views-field-body{
    margin-bottom: 0;
    float: right;
    width: 45%;
    font-family: $helvetica;
    color: #fff;
    padding-right: 50px;
    p{
      margin-bottom: 0;
