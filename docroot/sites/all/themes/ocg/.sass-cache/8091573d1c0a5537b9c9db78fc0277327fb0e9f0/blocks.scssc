 * @file
 * Block Styling
 */

@import "base";


.block { /* Block wrapper */
  margin-bottom: 1.5em;
  overflow: hidden;
  
  &.first { /* The first block in the region */
  }

  &.last { /* The last block in the region */
  }

  &.odd { /* Zebra striping for each block in the region */
  }

  &.even { /* Zebra striping for each block in the region */
  }

  h2.block-title { /* Block title */
  }
}


#block-aggregator-category-1 { /* Block for the latest news items in the first category */
}

#block-aggregator-feed-1 { /* Block for the latest news items in the first feed */
}

#block-block-1 { /* First block created with "Add block" link */
}

#block-blog-recent { /* "Recent blog posts" block */
}

#block-book-navigation { /* "Book navigation" block for the current book's table of contents */
}

#block-comment-recent { /* "Recent comments" block */
}

#block-forum-active { /* "Active forum topics" block */
}

#block-forum-new { /* "New forum topics" block */
}

#block-locale-language { /* Language switcher block */
}

#block-menu-menu-NAME { /* Custom menu block */
}

#block-node-recent { /* "Recent content" block */
}

#block-node-syndicate { /* "Syndicate" block for primary RSS feed; see also page.css's .feed-icon */
}

#block-poll-recent { /* "Most recent poll" block */
}

#block-profile-author-information { /* "Author information" block for the profile of the page's author */
}

.block-search,
#block-search-form { /* "Search form" block */
  float: right;
  width: 250px;
  input[type="submit"]{
    -webkit-appearance: none;
    background: none;
    border: none;
    font-family: $franklin;
    color: $callout-red;
    text-transform: uppercase;
    font-size: 14px;
    font-weight: normal;
    &:hover{
      text-decoration: underline;
    }
  }
  
  input[type="text"]{
    width: 150px;
    height: 20px;
    background-image: url(../images/search-input.gif);
    background-repeat: no-repeat;
    background-position: 5px 7px;
    padding: 3px 5px;
    color: $link-brown;
    border: 1px solid #cfcfcf;
    box-shadow: inset 0px 1px 3px #404040;
    padding-left: 25px;
    padding-top: 3px;
  }
  
  ::-webkit-input-placeholder {
     color: $link-brown;
  }
  
  :-moz-placeholder { /* Firefox 18- */
     color: $link-brown;  
  }
  
  ::-moz-placeholder {  /* Firefox 19+ */
     color: $link-brown;  
  }
  
  :-ms-input-placeholder {  
     color: $link-brown;  
  }
}

.block-search{
  float: none;
}

#block-shortcut-shortcuts { /* "Shortcuts" block */
}

#block-statistics-popular { /* "Popular content" block */
}

#block-system-main-menu{ /* "Main menu" block */
}

#block-system-management { /* "Management" block for Drupal management menu */
}

#block-system-navigation { /* "Navigation" block for Drupal navigation menu */
}

#block-system-user-menu { /* "User menu" block for Drupal user menu */
}

#block-system-help { /* "System help" block */
}

#block-system-main { /* "Main page content" block */
}

#block-system-powered-by { /* "Powered by Drupal" block */
}

#block-user-login { /* "User login form" block */
}

#block-user-new { /* "Who's new" block for a list of the newest users */
}

#block-user-online { /* "Who's online" block for a list of the online users */
}

.brown-block{
  z-index: 1;
  
  h2.block-title{
    color: $link-brown;
  }
  
  .view{
    margin-top: -4px;
    padding-top: 25px;
    position: relative;
    z-index: 1;
    
    &:before {
      content: "";
      position: absolute;
    	top: 0;
    	left: 0;
    	right: 0;
    	bottom: 0;
    	background-image: -webkit-gradient(radial, left top, 0, 100% 0%, 100, from(#9e9e9e), to(transparent));
    	background-image: -moz-radial-gradient(left top, farthest-corner, #9e9e9e 0%, transparent 72%);
    	z-index: -2;
    }
    
    &:after {
      content: "";
      position: absolute;
    	top: 1px;
    	left: 0;
    	bottom: 0;
    	right: 0;
    	background: #fff;
    	z-index: -1;
    }
  }
}

.grey-block{
  
  h2.block-title{
    color: #646464;
  }
  
  h3{
    font-size: 1.4em;
    line-height: 1.1em;
  }
  
  .view{
    margin-top: -4px;
    padding-top: 15px;
    position: relative;
    z-index: 1;
    overflow: visible;
    
    &:before {
      content: "";
      position: absolute;
    	top: 0;
    	left: 0;
    	right: 0;
    	bottom: 0;
    	background-image: -webkit-gradient(radial, left top, 0, 100% 0%, 100, from(#9e9e9e), to(transparent));
    	background-image: -moz-radial-gradient(left top, farthest-corner, #9e9e9e 0%, transparent 72%);
    	z-index: -2;
    }
    
    &:after {
      content: "";
      position: absolute;
    	top: 1px;
    	left: 0;
    	bottom: 0;
    	right: 0;
    	background: #fff;
    	z-index: -1;
    }
    
    .more-link{
      top: -15px;
      right: 0;
    } 
  }
  
  .view-projects{
    h3{
      font-size: 22px;
      line-height: 24px;
    }
  } 
}

.section-programs .grey-block.archive .view-news-and-publications{
  .views-row{
    border-bottom: none;
    margin-bottom: 7px !important;
  }
}

/*
 * Secondary Nav Menu Block Styles
 */
#block-menu-block-3{
  overflow: visible;
}

.secondary-nav{
  
  h2.block-title{
    font-size: 16.5px;
    color: $link-brown;
    line-height: 16px;
  }
  
  ul.menu{
    padding-left: 0;
    margin: 0;
    padding-top: 20px;
    border-top: 1px solid #dddddd;
    border-bottom: 1px solid #dddddd;
    font: $helvetica;
    font-weight: bold;
    
    li{
      padding-bottom: 7px;
      a{
       &:hover{
         color: $callout-red;
       } 
      }
      
      a.active-trail{
        color: $callout-red;
      }
    }
    
    li.collapsed{
      list-style-image: url(../images/has-children.gif);
    }
    
    li.expanded{
      list-style-image: url(../images/expanded.gif);
      list-style-type: none;
      ul li{
        list-style-image: none;
      }
    }
    
    li ul{
      border: none;
      padding: 0;
      
      li{
        padding: 5px 0 0 4px;
        font-weight: normal;
      }
      
    }
    
    a{
      text-decoration: none;
      color: #4b4b4b;
    }
  }
}

/*
 * Sidebar Second Block Styles
 */


.region-sidebar-second .corner{
  h2.block-title{
    font-size: $h5-font-size;
    font-weight: normal;
    color: #fff;
    padding: 7px 15px;
    background: rgb(121,98,69); /* Old browsers */
    background: -moz-linear-gradient(top,  rgba(121,98,69,1) 1%, rgba(123,102,76,1) 40%, rgba(98,79,56,1) 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(1%,rgba(121,98,69,1)), color-stop(40%,rgba(123,102,76,1)), color-stop(100%,rgba(98,79,56,1))); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top,  rgba(121,98,69,1) 1%,rgba(123,102,76,1) 40%,rgba(98,79,56,1) 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top,  rgba(121,98,69,1) 1%,rgba(123,102,76,1) 40%,rgba(98,79,56,1) 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top,  rgba(121,98,69,1) 1%,rgba(123,102,76,1) 40%,rgba(98,79,56,1) 100%); /* IE10+ */
    background: linear-gradient(to bottom,  rgba(121,98,69,1) 1%,rgba(123,102,76,1) 40%,rgba(98,79,56,1) 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#796245', endColorstr='#624f38',GradientType=0 ); /* IE6-8 */
    -ms-filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#796245', endColorstr='#624f38',GradientType=0 ); /* IE8+ */
  }
  
  .views-row{
    padding: 10px 15px;
    width: 190px;
    
    a{
      color: $black;
      text-decoration: none;
    }
  }
  
  p{
    margin-bottom: 7px;
    line-height: 19px;
  }
  
  .view{
   width:220px;
   min-height: 55px;
   float: left;
   margin-bottom: 30px;
   position: relative;
   overflow: hidden;
   background: #ffffff; /* Old browsers */
   background: -moz-linear-gradient(top,  #ffffff 0%, #f1f1f1 100%); /* FF3.6+ */
   background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#ffffff), color-stop(100%,#f1f1f1)); /* Chrome,Safari4+ */
   background: -webkit-linear-gradient(top,  #ffffff 0%,#f1f1f1 100%); /* Chrome10+,Safari5.1+ */
   background: -o-linear-gradient(top,  #ffffff 0%,#f1f1f1 100%); /* Opera 11.10+ */
   background: -ms-linear-gradient(top,  #ffffff 0%,#f1f1f1 100%); /* IE10+ */
   background: linear-gradient(to bottom,  #ffffff 0%,#f1f1f1 100%); /* W3C */
   filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#f1f1f1',GradientType=0 ); /* IE6-9 */
   -ms-filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#f1f1f1',GradientType=0 ); /* IE6-9 */
   &:before{
     content: "";
     position: absolute;
     bottom: 0;
     right: 0;
     border-width: 23px 23px 0 0;
     border-style: solid;
     border-color: transparent #fff transparent #fff;
     display: block;
     width: 0;
   }

    
    .views-row{
      margin-bottom: 5px;
      background: none;
      box-shadow: none;
      line-height: 16px;
      margin-bottom: 8px;
      
      &:before{
        content: none;
      }
      
      &:after{
        content: none;
      }
    }
    
    .views-row-even,
    .views-row-odd{
      padding-top: 0;
      padding-bottom: 0;
    }
    
    .views-row-first{
      padding-top: 10px;
    }
    
    .views-row-last{
      padding-bottom: 10px;
    }
  }
}

.region-sidebar-second .corner.email{
  h2{
    background: -moz-linear-gradient(top,  rgba(153,0,0,1) 0%, rgba(162,39,39,1) 40%, rgba(129,2,2,1) 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(153,0,0,1)), color-stop(40%,rgba(162,39,39,1)), color-stop(100%,rgba(129,2,2,1))); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top,  rgba(153,0,0,1) 0%,rgba(162,39,39,1) 40%,rgba(129,2,2,1) 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top,  rgba(153,0,0,1) 0%,rgba(162,39,39,1) 40%,rgba(129,2,2,1) 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top,  rgba(153,0,0,1) 0%,rgba(162,39,39,1) 40%,rgba(129,2,2,1) 100%); /* IE10+ */
    background: linear-gradient(to bottom,  rgba(153,0,0,1) 0%,rgba(162,39,39,1) 40%,rgba(129,2,2,1) 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#990000', endColorstr='#810202',GradientType=0 ); /* IE6-8 */
    -ms-filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#990000', endColorstr='#810202',GradientType=0 ); /* IE8+ */
  } 
}

.region-sidebar-second{
  .data-matrix{
    position: static;
  }
  
  .view-program-header .corner{
    margin-top: 0;
  }
}


.pubs{
  .view{
    img{
      box-shadow: 3px 5px 7px #b4b4b4;
    }
    
      .program-tag{
      float: none;
      display: inline-block;
    }
  }
}

/*
 * Utility Nav Styles
 */
#block-menu-menu-utility-menu{
  margin-bottom: 16px;
}
 
.utility{
  ul.menu{
  font-family: $franklin;
  text-transform: uppercase;
  font-size: 14px;
  margin-bottom: 0;
    li{
      float: left;
      padding-left: 12px;
      margin-right: 7px;
      background-image: url(../images/utility.png);
      background-repeat: no-repeat;
      background-position: left center;
      a{
        text-decoration: none;
        color: $link-brown;
        &:hover{
          color: $callout-red;
          text-decoration: underline;
        }
      }
    }
    
    li:first-child{
      background-image: none;
    }
  }
}

/*
 * Datasets Block Styles
 */

.datasets{
  font-family: $franklin;
  font-size: 16px;
  margin-bottom: 10px;
  p{
    margin-bottom: 0;
    a{
    color: $link-brown;
    text-decoration: none;
    font-weight: normal;
    display: block;
    background-image: url(../images/datasets-arrow.png);
    background-position: right center;
    background-repeat: no-repeat;
    padding-right: 25px;
    }
  }
}

#postscript-second .datasets{
  width: 250px;
  float: right;
  margin-top: 20px;
}

.program-datasets{
  position: absolute;
  top: 121.5px;
  right: 40px;
  width: 250px;
  z-index: 2;
  text-transform: uppercase;
  
  .ui-accordion-header{
    background-image: url(../images/datasets-diagonal.png) !important;
    background-repeat: repeat-x;
    background-position: left bottom;
    background-color: $callout-red !important;
    color: #fff;
    font-family: $franklin;
    font-size: 16px;
    border: none;
    background-image: none;
    border-radius: 0;
    padding: 0;
    padding-bottom: 5px;
    a{
      padding-left: 1em;
      padding-top: .6em;
      padding-bottom: .6em;
      color: #fff !important;
      &:hover{
        text-decoration: underline;
      }
    }
  }
  
  .ui-accordion-header.ui-state-active{
    background-image: none !important;
    padding-bottom: 0;
    color: #fff;
  }
  
  .ui-accordion-content{
    background-image: none;
    background: #614e37; /* Old browsers */
    background: -moz-linear-gradient(top,  #614e37 0%, #796245 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#614e37), color-stop(100%,#796245)); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top,  #614e37 0%,#796245 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top,  #614e37 0%,#796245 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top,  #614e37 0%,#796245 100%); /* IE10+ */
    background: linear-gradient(to bottom,  #614e37 0%,#796245 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#614e37', endColorstr='#796245',GradientType=0 ); /* IE6-9 */
    -ms-filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#614e37', endColorstr='#796245',GradientType=0 ); /* IE6-9 */
    border: none;
    border-radius: 0;
    padding: 10px 15px;
    height: auto !important;
    
    div{
      overflow: hidden;
      padding: 1px 0;
    }
    
    a{
      color: #fff;
      font-family: $franklin;
      text-decoration: none;
      font-size: 14px;
      padding: 10px 0;
      
      &:hover{
        text-decoration: underline;
      }
    }
  }
  
  h3 a{
    color: #fff;
  }
  
  span.ui-icon{
    background-image: url(../images/datasets-dropdown.png);
    background-repeat: no-repeat;
    background-position: left center;
    left: 14em !important;
    top: 28% !important;
    margin-top: 0 !important;
  }
}

.logged-in .program-datasets .view-access-datasets{
  position: absolute;
  top: -15px;
  right: 0;
  width: 250px;
  z-index: 2;
  text-transform: uppercase;
}

/*
 * Policy Block Styles
 */

#block-block-5{
  clear: both;
  margin-top: -50px;
}
 
.policy{
  font-size: 12px;
  color: #fff;
  padding-right: 20px;
  a{
    color: #fff;
    padding: 0 5px;
    text-decoration: none;
    font-weight: normal;
    &:hover{
      text-decoration: underline;
    }
  }
}

/*
 * NIH Block Styles
 */
.NIH{
  width: 360px;
  padding-left: 20px;

  a{
    float: left;
    margin-right: 20px;
    vertical-align: middle;
    img{
      vertical-align: middle;
    }
  }
  
  p{
    clear: both;
    color: #fff;
    font-size: 12px;
    padding-top: 10px;
  }
}

/*
 * Addthis Block Styles
 */

.addthis{
  font-family: $helvetica;
  font-size: 12px;
  float: right;
  margin-top: -45px;
  margin-bottom: 0;
  a{
    padding-left: 5px;
    margin-left: 5px;
    border-left: 1px dotted #d2d2d2;
    color: #323232;
    text-decoration: none;
  }
  
  .addthis_button_compact{
    border-left: none;
  }
}


/*
 * Lightbox Block Styles
 */
 
#cboxClose{
  top: 0;
  background: url(../images/x-button.png);
  background-repeat: no-repeat;
  background-position: 0 0;
  height: 32px;
  width: 32px;
  &:hover{
    background-position: 0 -32px;
  }
}
 
#block-views-programs-block-3{
  padding-right: 0;
  margin-right: 0;
} 

/*
 * Last Updated Block Styles
 */ 

#block-views-last-updated-block{
  clear: both;
}
 
/*
 * Collaborators Block Styles
 */ 
.view-program-sidebar{
  .views-field{
    margin-bottom: 8px;
  }
}




