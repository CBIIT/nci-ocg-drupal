}

#block-views-e-newsletter-block-1.right{
  padding-left: 0;
}

#block-views-publications-block-2.left{
  padding-right: 0;
}

.landing-page.view-e-newsletter{
  h2.block-title{
    font-size: $h5-font-size;
    font-weight: 700;
    margin-bottom: 10px;
    color: $link-brown;
    padding: 9px 15px 5px;
    border-bottom: 1px solid #fae21d;
    background: #faf4bf; /* Old browsers */
    background: -moz-linear-gradient(top,  #faf4bf 0%, #fef6d5 30%, #faec7a 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#faf4bf), color-stop(30%,#fef6d5), color-stop(100%,#faec7a)); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top,  #faf4bf 0%,#fef6d5 30%,#faec7a 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top,  #faf4bf 0%,#fef6d5 30%,#faec7a 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top,  #faf4bf 0%,#fef6d5 30%,#faec7a 100%); /* IE10+ */
    background: linear-gradient(to bottom,  #faf4bf 0%,#fef6d5 30%,#faec7a 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#faf4bf', endColorstr='#faec7a',GradientType=0 ); /* IE6-9 */
    .fontface & {
      font-family: $franklin;
      font-weight: normal;
    }
  }
  
  .views-row .field-content{
    padding: 5px 20px;
    margin-bottom: 0;
  }
  
  .views-row .views-field-field-issue-date .field-content{
    padding: 0;
  }
  
  p.field-content{
    font-family: $helvetica;
  }
  
  h3.field-content{
    color: #767676;
  }
  
  .views-field-title-1 a{
    color: $callout-red !important;
  }
  
  .views-field-title p{
    color: $link-brown;
    font-size: 15px;
    font-family: $arial;
    font-weight: 700;
    .fontface & {
      font-family: $franklin;
      font-weight: normal;
    }
  }
  
  .views-row{
    padding-bottom: 20px;
  }
}

/*
 * Resources Archive View Styles
 */

.views-field-field-resource-link{
  padding-bottom: 10px;
  h3{
    font-size: $h4-font-size;
  }
}

/*
 * Last Updated View Styles
 */

#block-views-last-updated-block{
  position: absolute;
  bottom: 30px;
  .view-last-updated{
    font-family: $helvetica;
    font-size: 12px;
    color: #646464;
  }
}

/*
 * Program tag with Abbreviated Title View Styles
 */

.view-program-abbreviation{
  float: left;
  clear: both;
  color: #7d7d7d;
  font-weight: bold;
  text-transform: uppercase;
  padding: 5px 10px 5px 0;
  a{
    width: 70px;
    display: inline-block;
    background-color: #767676;
    text-align: center;
    color: #fff !important;
    font-weight: bold;
    margin-right: 10px; 
    &:hover{
      color: #fff !important;
    }
  }
}


/*
 * Individual E-Newsletter View Styles
 */
.view-taxonomy-term{
  .view-content{
    .views-row{
      margin-bottom: 10px;
      padding-bottom: 15px;
      border-bottom: 5px solid #e1e1e1;
      overflow: visible;
      img{
        float: left;
        padding-right: 15px;
        padding-bottom: 10px;
      }
      .views-field-title{
        h2{
          color: $callout-red;
          font-size: 22px;
          line-height: 25px;
            span{
            color: #646464;
            font-size: 26px;
            margin-bottom: 6px;
            text-transform: uppercase;
          }
        }
      }
    }
    .views-row-last{
      border-bottom: none;
    }
  }
  .attachment-before{
    margin-bottom: 10px;
    padding-bottom: 10px;
    border-bottom: 5px solid #e1e1e1;
    .views-row{
      margin-bottom: 0;
      padding-bottom: 0;
      border-bottom: none;
    }
    h3{
      margin-bottom: 22px;
      span{
        color: #646464;
        font-size: 21px;
        margin-bottom: 6px;
        text-transform: uppercase;
      }
      a{
        color: $callout-red !important;
        font-size: 19px;
        line-height: 23px;
      }
    }
  }
  .views-field-field-enews-physician{
    font-size: 12px;
    line-height: 18px;
    margin-bottom: 10px;
  }
  .view-e-newsletter-issue-title{
    .views-row{
      padding-bottom: 0;
      h1{
        color: $link-brown;
        span{
          color: $callout-red;
        }
      }
    }
  }
}











