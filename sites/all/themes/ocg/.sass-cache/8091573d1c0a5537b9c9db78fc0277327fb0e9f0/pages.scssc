}

.item-list .pager { /* A list of page numbers when more than 1 page of content is available */
  padding: 0;

  li { /* Each page number in the pager list */
    padding: 0 10px 0 0;
    margin-right: 5px;
    text-transform: capitalize;
  }
  
  li.pager-current,
  li.pager-item{
    border-right: 1px solid #d2d2d2;
  }
  
